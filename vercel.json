{
  "version": 2,
  "builds": [{
    "src": "./index.js",
    "use": "@vercel/node"

  }],
  "routes": [{
      "src": "/(.*)",
      "dest": "/",
      "methods": ["*"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/",
      "dest": "/addBooking",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/",
      "dest": "/getBooking",
      "methods": ["GET"],
      "headers": {
      "Access-Control-Allow-Origin": "*"
    } 
    }
  ]
}