{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
       
      }
    ],
    "routes": [
      { "src": "/(.*)", 
      "dest": "/" ,
    "methods": ["*"],
    "headers": {
      "Access-Control-Allow-Origin": "*"
    } 
    },
    {
      "src": "/addBooking",  
      "dest": "/addBooking", 
      "methods": ["POST"]    
    },
    {
      "src": "/getBooking",  
      "dest": "/getBooking", 
      "methods": ["GET"]    
    }
    ]
  }